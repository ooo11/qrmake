{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "title": "QR-Image",
  "description": "The qr-Image JSON document.",
  "required": ["qr_img_url", "students_id"],
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The id of an qr image."
    },
    "qr_img_url": {
      "$id": "#/properties/qr_img_url",
      "type": "string",
      "title": "The url of an qr image."
    },
    "students_id": {
      "$id": "#/properties/students_id",
      "type": "integer",
      "title": "The id of the students"
    },
    "created_at": {
      "$id": "#/properties/created_at",
      "type": "string",
      "title": "The creation date of the qr image."
    },
    "updated_at": {
      "$id": "#/properties/updated_at",
      "type": "string",
      "title": "The date the qr image last updated."
    }
  },
  "additionalProperties": false
}
