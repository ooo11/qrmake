{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "title": "Students",
  "description": "The students JSON document.",
  "required": ["name","classnames_id"],
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The id of a student."
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "Name of student."
    },
    "classnames_id": {
      "$id": "#/properties/classNames_id",
      "type": "number",
      "title": "The id for the class of the student."
    },
    "created_at": {
      "$id": "#/properties/created_at",
      "type": "string",
      "title": "The creation date of the item."
    },
    "updated_at": {
      "$id": "#/properties/updated_at",
      "type": "string",
      "title": "The date the item last updated."
    }
  },
  "additionalProperties": false
}
